# Where should I put you?
bindkey -s ^f "tmux-sessionizer\n"

# folders
alias go."cd $HOME/.dotfiles/"
alias goc="cd $HOME/.dotfiles/.config/"
alias gon="cd $HOME/.dotfiles/.config/nvim/"
alias got="cd $HOME/.dotfiles/.config/tmux/"
alias gob="cd $HOME/.dotfiles/.local/bin/"
alias gog="cd $HOME/.git/"
alias gop="cd $HOME/.git/python/"
alias god="cd $HOME/.git/python/dsa/"



# edits
alias ez="nvim $HOME/.dotfiles/.zsh_profile"
alias eh="nvim $HOME/.dotfiles/.config/hypr"


# git
alias ac="git add . && git commit -m 'updated...'"
alias gp='git push origin main'

# shorts
alias cm="chmod +x"
alias ls='lsd'

# creat fzf to select movies in videos folder
# preview with mpv
alias fz='fzf --preview="mpv --no-video-title-show {}"'
